{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.5.0"
  },
  "devDependencies": {
    "canonical-json": "0.0.4",
    "handlebars": "4.0.11",
    "hexo": "3.5.0",
    "hexo-generator-archive": "0.1.5",
    "hexo-generator-category": "0.1.3",
    "hexo-generator-index": "0.2.1",
    "hexo-generator-tag": "0.2.0",
    "hexo-renderer-ejs": "0.3.1",
    "hexo-renderer-less": "0.2.0",
    "hexo-renderer-marked": "0.3.2",
    "hexo-renderer-stylus": "0.3.3",
    "hexo-s3-deploy": "1.2.2",
    "hexo-server": "0.3.1",
    "hexo-versioned-netlify-redirects": "1.0.2",
    "jsdoc": "3.5.5",
    "meteor-hexo-config": "1.0.5",
    "meteor-theme-hexo": "1.0.1",
    "showdown": "1.8.6",
    "underscore": "1.8.3"
  },
  "scripts": {
    "build": "jsdoc/jsdoc.sh && hexo generate --config ./node_modules/meteor-hexo-config/_config.yml,./_config.yml",
    "clean": "hexo clean; rm data/data.js data/names.json",
    "test": "npm run clean; npm run build",
    "predeploy": "npm run build",
    "deploy": "hexo-s3-deploy",
    "start": "npm run build && hexo server --config ./node_modules/meteor-hexo-config/_config.yml,./_config.yml"
  }
}
